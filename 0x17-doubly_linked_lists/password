#include <stdio.h>

int main() {
    char username[50], password[50];
    FILE *fp = fopen("crackme4", "r");
    if (fp == NULL) {
        printf("Error opening file\n");
        return 1;
    }
    fscanf(fp, "%s %s", username, password);
    printf("Username: %s\nPassword: %s\n", username, password);
    fclose(fp);
    return 0;
}
